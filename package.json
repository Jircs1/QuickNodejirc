{
    "name": "quicknode",
    "version": "1.1.2",
    "author": "devilyouwei",
    "keywords": [
        "quicknode",
        "qbite",
        "RESTful",
        "API",
        "JSON",
        "web"
    ],
    "description": "A simple and easy way for you to create RESTful API on Node.js.",
    "main": "server.js",
    "dependencies": {
        "express": "latest",
        "stripe": "latest",
        "mysql2": "latest",
        "connect-multiparty": "latest",
        "trim": "latest",
        "htmlspecialchars": "latest",
        "md5": "latest",
        "ali-oss": "latest",
        "nodemailer": "latest",
        "nodemailer-smtp-transport": "latest",
        "superagent": "latest",
        "app-root-path": "latest"
    },
    "devDependencies": {
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "prettier": "^1.19.1"
    },
    "eslintConfig": {
        "root": true,
        "parserOptions": {
            "ecmaVersion": 2017,
            "parser": "babel-eslint"
        },
        "env": {
            "es6": true,
            "node": true
        },
        "plugins": [
            "prettier"
        ],
        "extends": [
            "eslint:recommended",
            "plugin:prettier/recommended"
        ],
        "rules": {
            "no-console": "off",
            "no-unused-vars": "warn",
            "prettier/prettier": [
                "error"
            ]
        }
    },
    "license": "MIT"
}
