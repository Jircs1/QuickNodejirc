{
    "name": "quicknode",
    "version": "2.0.0",
    "author": "devilyouwei",
    "keywords": [
        "QuickNode",
        "nodejs",
        "RESTful",
        "API",
        "web framework",
        "Convention Routing",
        "MVC"
    ],
    "description": "A simple and easy way for you to create RESTful API on Node.js.",
    "main": "server.js",
    "scripts": {
        "dev": "pm2-dev start server.js",
        "restart": "pm2 restart server.js",
        "start": "pm2 start server.js",
        "stop": "pm2 stop server.js",
        "status": "pm2 status",
        "delete": "pm2 delete app.js",
        "pm2-update": "pm2 update"
    },
    "dependencies": {
        "@stdlib/assert-is-json": "^0.1.1",
        "ali-oss": "^6.20.0",
        "app-root-path": "^3.1.0",
        "axios": "^1.6.5",
        "connect-multiparty": "^2.2.0",
        "dicom-parser": "^1.8.21",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "htmlspecialchars": "^1.0.5",
        "md5": "^2.3.0",
        "mysql2": "^3.7.1",
        "pm2": "^5.3.0",
        "sequelize": "^6.35.2",
        "trim": "^1.0.1"
    },
    "devDependencies": {
        "@types/ali-oss": "^6.16.11",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "prettier": "^1.19.1"
    },
    "eslintConfig": {
        "root": true,
        "parserOptions": {
            "ecmaVersion": 2017,
            "parser": "babel-eslint"
        },
        "env": {
            "es6": true,
            "node": true
        },
        "plugins": [
            "prettier"
        ],
        "extends": [
            "eslint:recommended",
            "plugin:prettier/recommended"
        ],
        "rules": {
            "no-console": "off",
            "no-unused-vars": "warn",
            "prettier/prettier": [
                "error"
            ]
        }
    },
    "prettier": {
        "trailingComma": "none",
        "printWidth": 120,
        "tabWidth": 4,
        "semi": false,
        "bracketSpacing": true,
        "htmlWhitespaceSensitivity": "ignore",
        "arrowParens": "avoid",
        "singleQuote": true,
        "endOfLine": "auto"
    },
    "license": "MIT"
}
